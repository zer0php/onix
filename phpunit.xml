<phpunit
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/9.1/phpunit.xsd"
    bootstrap="vendor/autoload.php"
    cacheResult="false"
    failOnWarning="true"
    testdox="true"
    >
    <testsuite name="App">
        <directory>test/</directory>
    </testsuite>
    <filter>
        <whitelist>
            <directory>src/</directory>
        </whitelist>
    </filter>
</phpunit>
